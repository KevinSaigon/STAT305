# print(negativeLogLikelihood(1))
# print(negativeLogLikelihood(2))
# print(negativeLogLikelihood(3))
# print(negativeLogLikelihood(4))
# print(negativeLogLikelihood(5))
fit <- mle((negativeLogLikelihood), start = list(alpha = 1), method = "L-BFGS-B", lower = 1, upper = 200)
